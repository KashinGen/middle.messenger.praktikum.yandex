{
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "curly": "error",
      "no-console": ["warn", { "allow":  ["error"]}],
      "no-useless-rename": "error"
    }
}
  